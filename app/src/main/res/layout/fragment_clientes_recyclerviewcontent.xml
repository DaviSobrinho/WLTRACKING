<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


        <TextView
            android:id="@+id/Fragment_clientes_nome"
            android:layout_width="100dp"
            android:layout_height="40dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"
            android:clickable="true"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_clientes_cpf"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/Fragment_clientes_nome"
            android:clickable="true"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_clientes_data"
            android:layout_width="100dp"
            android:layout_height="40dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/Fragment_clientes_cpf"
            android:clickable="true"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_clientes_telefone"
            app:layout_constraintTop_toBottomOf="@id/Fragment_clientes_nome"
            app:layout_constraintStart_toStartOf="@id/Fragment_clientes_nome"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:clickable="true"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_clientes_valor"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:clickable="true"
            app:layout_constraintTop_toBottomOf="@id/Fragment_clientes_cpf"
            app:layout_constraintStart_toEndOf="@id/Fragment_clientes_nome"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_clientes_contrato"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:clickable="true"
            app:layout_constraintTop_toBottomOf="@id/Fragment_clientes_data"
            app:layout_constraintStart_toEndOf="@id/Fragment_clientes_valor"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_clientes_notas"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:clickable="true"
            app:layout_constraintTop_toBottomOf="@id/Fragment_clientes_telefone"
            app:layout_constraintStart_toStartOf="@id/Fragment_clientes_telefone"
            android:textColor="@color/black"
            android:textSize="24sp"/>


</androidx.constraintlayout.widget.ConstraintLayout>