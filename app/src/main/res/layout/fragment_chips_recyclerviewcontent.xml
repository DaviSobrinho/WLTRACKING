<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


        <TextView
            android:id="@+id/Fragment_chips_Imei"
            android:text="teste"
            android:layout_width="100dp"
            android:layout_height="40dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/black"
            android:clickable="true"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_chips_Estado"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/Fragment_chips_Imei"
            android:clickable="true"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_chips_Modelo"
            android:layout_width="100dp"
            android:layout_height="40dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/Fragment_chips_Estado"
            android:clickable="true"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_chips_Telefone"
            app:layout_constraintTop_toBottomOf="@id/Fragment_chips_Imei"
            app:layout_constraintStart_toStartOf="@id/Fragment_chips_Imei"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:clickable="true"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_chips_Custo"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:clickable="true"
            app:layout_constraintTop_toBottomOf="@id/Fragment_chips_Imei"
            app:layout_constraintStart_toEndOf="@id/Fragment_chips_Telefone"
            android:textColor="@color/black"
            android:textSize="24sp"/>
        <TextView
            android:id="@+id/Fragment_chips_Dia"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:clickable="true"
            app:layout_constraintTop_toBottomOf="@id/Fragment_chips_Imei"
            app:layout_constraintStart_toEndOf="@id/Fragment_chips_Custo"
            android:textColor="@color/black"
            android:textSize="24sp"/>



</androidx.constraintlayout.widget.ConstraintLayout>